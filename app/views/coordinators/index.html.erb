<h1>Listing coordinators</h1>

<table>
  <tr>
    <th>Name</th>
    <th>City</th>
    <th>Photo file name</th>
    <th>Photo content type</th>
    <th>Photo file size</th>
    <th>User</th>
  </tr>

<% for coordinator in @coordinators %>
  <tr>
    <td><%=h coordinator.name %></td>
    <td><%=h coordinator.city %></td>
    <td><%=h coordinator.photo_file_name %></td>
    <td><%=h coordinator.photo_content_type %></td>
    <td><%=h coordinator.photo_file_size %></td>
    <td><%=h coordinator.user_id %></td>
    <td><%= link_to 'Show', coordinator %></td>
    <td><%= link_to 'Edit', edit_coordinator_path(coordinator) %></td>
    <td><%= link_to 'Destroy', coordinator, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New coordinator', new_coordinator_path %>

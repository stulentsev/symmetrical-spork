<h1>Listing student_professional_profiles</h1>

<table>
  <tr>
    <th>Student</th>
  </tr>

<% for student_professional_profile in @student_professional_profiles %>
  <tr>
    <td><%=h student_professional_profile.student_id %></td>
    <td><%= link_to 'Show', student_professional_profile %></td>
    <td><%= link_to 'Edit', edit_student_professional_profile_path(student_professional_profile) %></td>
    <td><%= link_to 'Destroy', student_professional_profile, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New student_professional_profile', new_student_professional_profile_path %>

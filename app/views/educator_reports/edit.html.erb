<h1>Editing educator_report</h1>

<% form_for(@educator_report) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :content_description %><br />
    <%= f.text_area :content_description %>
  </p>
  <p>
    <%= f.label :methods_used %><br />
    <%= f.text_area :methods_used %>
  </p>
  <p>
    <%= f.label :observations %><br />
    <%= f.text_area :observations %>
  </p>
  <p>
    <%= f.label :trimester_id %><br />
    <%= f.text_field :trimester_id %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @educator_report %> |
<%= link_to 'Back', educator_reports_path %>

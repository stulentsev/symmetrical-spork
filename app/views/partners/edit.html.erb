<h1>Editing partners</h1>

<% form_for(@partners) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :purpose %><br />
    <%= f.text_area :purpose %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @partners %> |
<%= link_to 'Back', partners_path %>

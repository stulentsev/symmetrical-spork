<h1>Listing project_team_members</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Function</th>
    <th>Competencies</th>
    <th>Curriculum summary</th>
    <th>Email</th>
  </tr>

<% for project_team_member in @project_team_members %>
  <tr>
    <td><%=h project_team_member.name %></td>
    <td><%=h project_team_member.function %></td>
    <td><%=h project_team_member.competencies %></td>
    <td><%=h project_team_member.curriculum_summary %></td>
    <td><%=h project_team_member.email %></td>
    <td><%= link_to 'Show', project_team_member %></td>
    <td><%= link_to 'Edit', edit_project_team_member_path(project_team_member) %></td>
    <td><%= link_to 'Destroy', project_team_member, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New project_team_member', new_project_team_member_path %>

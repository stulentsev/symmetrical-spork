<h1>Editing project_team_member</h1>

<% form_for(@project_team_member) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :function %><br />
    <%= f.text_field :function %>
  </p>
  <p>
    <%= f.label :competencies %><br />
    <%= f.text_area :competencies %>
  </p>
  <p>
    <%= f.label :curriculum_summary %><br />
    <%= f.text_area :curriculum_summary %>
  </p>
  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @project_team_member %> |
<%= link_to 'Back', project_team_members_path %>
